{
  "rewrites": [
    {
      "source": "/api/:path*/",
      "destination": "https://api.mysterria.net/api/:path*"
    },
    {
      "source": "/api/:path*",
      "destination": "https://api.mysterria.net/api/:path*"
    },
    {
      "source": "/plan/:path*",
      "destination": "https://catwalk.mysterria.net/:path*"
    },
    {
      "source": "/catwalk/:path*",
      "destination": "https://catwalk.mysterria.net/:path*"
    },
    {
      "source": "/auth/:path*",
      "destination": "/index.html"
    },
    {
      "source": "/game",
      "destination": "/index.html"
    },
    {
      "source": "/rules",
      "destination": "/index.html"
    },
    {
      "source": "/shop",
      "destination": "/index.html"
    },
    {
      "source": "/profile/:path*",
      "destination": "/index.html"
    },
    {
      "source": "/users",
      "destination": "/index.html"
    },
    {
      "source": "/logout",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/sentry",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Access-Control-Allow-Methods", "value": "POST, OPTIONS" },
        { "key": "Access-Control-Allow-Headers", "value": "Content-Type, X-Sentry-Auth, Origin" }
      ]
    }
  ]
}